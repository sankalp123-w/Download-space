RewriteEngine On

RewriteRule ^music/(.+)$ music.php?v=$1
RewriteRule ^vid/(.+)$ vid.php?v=$1

RewriteRule ^watch/(.+)$ watch.php?v=$1
RewriteRule ^contact-us/$ contact-us.php
RewriteRule ^watch$ watch.php
Options -Indexes
ErrorDocument 402 /index.php
ErrorDocument 403 /index.php
ErrorDocument 404 /index.php
ErrorDocument 500 /index.php

